{
    "version": "v1",
    "notes": "Deterministic archetype scoring. Use q1/q2/q3/product_type. Highest score wins. Tie-break with inputHash.",
    "global": {
        "normalize": {
            "lowercase": true,
            "strip_punctuation": true,
            "collapse_whitespace": true
        },
        "fluff_dampeners": [
            {
                "pattern": "\\b(revolutionary|ground[- ]?breaking|cutting[- ]?edge|seamless|robust|next[- ]?gen|world[- ]?class|best[- ]?in[- ]?class|innovative|game[- ]?changing)\\b",
                "multiplier": 0.6
            }
        ]
    },
    "archetypes": {
        "The Outlaw": {
            "description": "Contrarian, anti-status-quo, provocative, rule-breaking.",
            "signals": [
                {
                    "type": "phrase",
                    "pattern": "break the rules",
                    "weight": 7
                },
                {
                    "type": "phrase",
                    "pattern": "no more",
                    "weight": 4
                },
                {
                    "type": "phrase",
                    "pattern": "stop doing",
                    "weight": 4
                },
                {
                    "type": "regex",
                    "pattern": "\\b(disrupt|disruption|rebel|rebellious|outlaw|contrarian|anti[- ]?status[- ]?quo)\\b",
                    "weight": 6
                },
                {
                    "type": "regex",
                    "pattern": "\\b(revolutionize|ground[- ]?breaking|smash|kill|tear down|burn down|wreck|flip|against)\\b",
                    "weight": 5
                },
                {
                    "type": "regex",
                    "pattern": "\\b(unapologetic|ruthless|brutal honesty|no bs|no-bs)\\b",
                    "weight": 4
                }
            ],
            "anti_signals": [
                {
                    "type": "regex",
                    "pattern": "\\b(compliance|governance|policy|regulated)\\b",
                    "weight": -3
                }
            ]
        },
        "The Hero": {
            "description": "Achievement, overcoming obstacles, performance, winning, shipping, discipline.",
            "signals": [
                {
                    "type": "regex",
                    "pattern": "\\b(win|winning|beat|crush|dominate|conquer|overcome|battle|grind|disciplined|discipline)\\b",
                    "weight": 5
                },
                {
                    "type": "regex",
                    "pattern": "\\b(ship|launch|finish|complete|execute)\\b",
                    "weight": 4
                },
                {
                    "type": "regex",
                    "pattern": "\\b(stronger|faster|harder|tough|stamina|endurance|performance)\\b",
                    "weight": 4
                },
                {
                    "type": "regex",
                    "pattern": "\\b(fitness|training|workout|marathon|run|gym)\\b",
                    "weight": 3
                }
            ],
            "anti_signals": [
                {
                    "type": "regex",
                    "pattern": "\\b(cozy|gentle|soft|comfort)\\b",
                    "weight": -2
                }
            ]
        },
        "The Magician": {
